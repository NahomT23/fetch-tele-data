{
    "version": 2,
    "builds": [
      {
        "src": "frontend/botCommerce/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      },
      {
        "src": "backend/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/backend/(.*)",
        "dest": "/backend/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/botCommerce/$1"
      }
    ]
  }
  